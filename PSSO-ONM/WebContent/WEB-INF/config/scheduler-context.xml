				<property name="baseDir" value="${baseDir.healthcheck}" />
				<property name="logName" value="${logName.holder}" />
			</bean>
		</property>
		<property name="siteCode" value="${holder.siteCode}" />
		<property name="jumin" value="${holder.jumin}" />
		<property name="mobileCo" value="${holder.mobileCo}" />
		<property name="mobileNo" value="${holder.mobileNo}" />
