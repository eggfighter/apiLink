	<bean id="ipinHealthChecker" class="com.kt.psso.onm.scheduler.hc.IpinHealthChecker">
		<property name="writer">
			<bean class="com.kt.psso.onm.scheduler.hc.LineWriter">
				<property name="baseDir" value="${baseDir.healthcheck}" />
				<property name="logName" value="${logName.ipin}" />
			</bean>
		</property>
		<property name="userId" value="${ipin.userId}" />
		<property name="host" value="${ipin.host}" />
		<property name="port" value="${ipin.port}" />
		<property name="timeout" value="${ipin.timeout}" />
		<property name="docCode" value="${ipin.docCode}" />
		<property name="jobCode" value="${ipin.jobCode}" />
		<property name="returnCode" value="${ipin.returnCode}" />
		<property name="jumin" value="${ipin.jumin}" />
		<property name="code" value="${ipin.code}" />
	</bean>

